# Haiku Kernel Driver Jamfile for Intel i915

SetOptional KernelAddonFlags DEBUG ;

KernelAddon intel_i915 :
	intel_i915.c
	gtt.c
	irq.c
	vbt.c
	gmbus.c
	edid.c
	clocks.c
	display.c
	gem_object.c
	gem_ioctl.c
	engine.c
	gem_context.c
	# Add other source files here as they are created.
	# Ensure intel_i915_priv.h is correctly used by these.
	: libkernel.a
;
